<h1>All Places</h1>

<div class="row">
  <% @places.each do |place| %>
    <div class="col-md-4">
      <div class="card mb-3">
      

        <div class="card-body">
          <h5 class="card-title"><%= place.name %></h5>
          <a href="/places/<%= place.id %>" class="btn btn-primary">View Details</a>
        </div>
      </div>
    </div>
  <% end %>
</div>